{% extends '::base.html.twig' %}

{% block title %}
    Horoscope
{% endblock %}

{% block body %}
    <h1>Horoscope Zodiac</h1>

    <p>Essai contenu page horoscope dans bundle</p>
         {% for zodiac in zodiacData %}
             <div class="pull-left panel panel-primary" style="width:250px;margin-right: 15px">
                <div class="panel-heading">
                    <h3 class="panel-title">{{ zodiac.name }}</h3>
                </div>
                <div class="panel-body">
                    <a href="{{ path('jdf_horoscope_zodiac', { 'id': zodiac.id ,'label' : 'test'}) }}">{{ zodiac.name }}</a>
                </div>
              </div>
             
         {% endfor%}
{% endblock %}



